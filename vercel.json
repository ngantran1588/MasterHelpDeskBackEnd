{
  "version": 2,
  "builds": [
    { "src": "main.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "functions": {
    "main.py": {
      "maxDuration": 60
    },
    "src/api/*.py": {
      "maxDuration": 60
    },
    "src/**/*": {
      "maxDuration": 60
    }
  }  
}